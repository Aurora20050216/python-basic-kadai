class Human:
  def __init__(self,name,age):
    self.name = name
    self.age = age

  def check_adult(self):
    if self.age >= 20:
      print("大人です")
    else:
      print("大人ではありません")

humans = []
humans.append(Human("太郎",27))
humans.append(Human("次郎",23))
humans.append(Human("三郎",19))
humans.append(Human("四郎",17))

for human in humans:
  human.check_adult()
